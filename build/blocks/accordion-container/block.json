{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "rs-blockington/accordion-container",
  "title": "Accordion Container",
  "category": "rs-blockington-bootstrap-blocks",
  "description": "Groups accordion items inside a container.",
  "textdomain": "rs-blockington",
  "attributes": {
    "accordion_color": {
      "type": "string",
      "default": "#212529"
    },
    "accordion_bg": {
      "type": "string",
      "default": "#fff"
    },
    "accordion_border_color": {
      "type": "string",
      "default": "var(--bs-border-color)"
    },
    "accordion_border_width": {
      "type": "string",
      "default": "1px"
    },
    "accordion_border_radius": {
      "type": "string",
      "default": "0.375rem"
    },
    "accordion_inner_border_radius": {
      "type": "string",
      "default": "calc(0.375rem - 1px)"
    },
    "accordion_btn_padding_x": {
      "type": "string",
      "default": "1.25rem"
    },
    "accordion_btn_padding_y": {
      "type": "string",
      "default": "1rem"
    },
    "accordion_btn_color": {
      "type": "string",
      "default": "#212529"
    },
    "accordion_btn_bg": {
      "type": "string",
      "default": "var(--bs-accordion-bg)"
    },
    "accordion_btn_focus_border_color": {
      "type": "string",
      "default": "#86b7fe"
    },
    "accordion_body_padding_x": {
      "type": "string",
      "default": "1.25rem"
    },
    "accordion_body_padding_y": {
      "type": "string",
      "default": "1rem"
    },
    "accordion_active_color": {
      "type": "string",
      "default": "#0c63e4"
    },
    "accordion_active_bg": {
      "type": "string",
      "default": "#e7f1ff"
    },
    "accordion_icon_color": {
      "type": "string",
      "default": "#e7f1ff"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./index.css"
}